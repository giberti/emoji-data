{
    "name": "giberti/emoji-data",
    "description": "PHP classes with constant to multibyte emoji characters",
    "license": "MIT",
    "type": "library",
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "nicmart/tree": "^0.3.0",
        "phpunit/phpunit": "^9.3",
        "twig/twig": "^3.0"
    },
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Giberti\\EmojiData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Giberti\\EmojiDataBuilder\\": "builder/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    }
}
